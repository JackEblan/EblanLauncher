<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="Theme.EblanLauncher" parent="android:Theme.Material.Light.NoActionBar" />

    <style name="Theme.EblanLauncher.Wallpaper" parent="android:Theme.Material.Light.NoActionBar">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowShowWallpaper">true</item>
        <item name="android:windowLightStatusBar">true</item>
        <item name="android:windowLightNavigationBar" tools:targetApi="27">true</item>
        <item name="android:navigationBarColor" tools:targetApi="27">@android:color/transparent
        </item>
        <item name="android:statusBarColor">@android:color/transparent</item>
        <item name="android:enforceStatusBarContrast" tools:targetApi="29">false</item>
        <item name="android:enforceNavigationBarContrast" tools:targetApi="29">false</item>
    </style>

</resources>